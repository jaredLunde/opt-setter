{
  "name": "opt-setter",
  "version": "0.0.6-alpha",
  "main": "index.js",
  "private": true,
  "dependencies": {
    "object-assign": "^4.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.11.6",
    "cross-env": "^2.0.0",
    "strip-loader": "^0.1.2",
    "webpack": "^2.1.0-beta.20",
    "webpack-dashboard": "0.0.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^2.1.0-beta.0",
    "webpack-hot-middleware": "^2.12.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jaredlunde/opt-setter"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development node server.js",
    "build": "npm run build:es5 && npm run build:es6",
    "build:es5": "cross-env NODE_ENV=cli babel ./lib --out-dir es5",
    "build:es6": "cross-env NODE_ENV=production babel ./lib --out-dir ./",
    "build:dist": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "prepublish": "npm run build"
  },
  "analyze": true,
